"generated":{"line":59,"column":5},"source":"services/upload.js","original":{"line":52,"column":8}},{"name":"busboy","generated":{"line":60,"column":0},"source":"services/upload.js","original":{"line":57,"column":8}},{"name":"busboy","generated":{"line":60,"column":4},"source":"services/upload.js","original":{"line":57,"column":8}},{"generated":{"line":60,"column":10},"source":"services/upload.js","original":{"line":57,"column":14}},{"name":"on","generated":{"line":60,"column":11},"source":"services/upload.js","original":{"line":57,"column":15}},{"generated":{"line":60,"column":13},"source":"services/upload.js","original":{"line":57,"column":8}},{"generated":{"line":60,"column":14},"source":"services/upload.js","original":{"line":57,"column":18}},{"generated":{"line":60,"column":21},"source":"services/upload.js","original":{"line":57,"column":8}},{"name":"reject","generated":{"line":60,"column":23},"source":"services/upload.js","original":{"line":57,"column":27}},{"generated":{"line":60,"column":29},"source":"services/upload.js","original":{"line":57,"column":8}},{"generated":{"line":61,"column":0},"source":"services/upload.js","original":{"line":58,"column":5}},{"generated":{"line":61,"column":3},"source":"services/upload.js","original":{"line":51,"column":11}},{"generated":{"line":61,"column":4},"source":"services/upload.js","original":{"line":51,"column":4}},{"generated":{"line":62,"column":0},"source":"services/upload.js","original":{"line":59,"column":1}}],"sources":{"services/upload.js":"const Busboy = require('busboy');\r\n\r\nexport function upload(){\r\n    const busboy = new Busboy(busboyOptions);\r\n    const tmpdir = os.tmpdir();\r\n\r\n    const fields = {};\r\n    const uploads = {};\r\n\r\n    // This code will process each non-file field in the form.\r\n    busboy.on('field', (field, val) => {\r\n        console.log(`Processed field ${field}: ${val}.`);\r\n        fields[field] = val;\r\n    });\r\n\r\n    const fileWrites = [];\r\n\r\n    // This code will process each file uploaded.\r\n    busboy.on('file', (field, file, name) => {\r\n        // Note: os.tmpdir() points to an in-memory file system on GCF\r\n        // Thus, any files in it must fit in the instance's memory.\r\n        if (dev) console.log(`Processed file ${field} ${name}`);\r\n        const filepath = path.join(tmpdir, name);\r\n        if (field.indexOf('[]') !== -1) {\r\n            field = field.split('[]').join('')\r\n            if (Array.isArray(uploads[field])) {\r\n                if (dev) console.log(`Array detected`);\r\n                uploads[field].push(filepath);\r\n            } else {\r\n                if (dev) console.log(`Creating array`, uploads[field]);\r\n                uploads[field] = [filepath];\r\n            }\r\n        } else {\r\n            uploads[field] = filepath;\r\n        }\r\n\r\n        const writeStream = fs.createWriteStream(filepath);\r\n        file.pipe(writeStream);\r\n\r\n        // File was processed by Busboy; wait for it to be written to disk.\r\n        const promise = new Promise((resolve, reject) => {\r\n            file.on('end', () => {\r\n                writeStream.end();\r\n            });\r\n            writeStream.on('finish', resolve);\r\n            writeStream.on('error', reject);\r\n        });\r\n        fileWrites.push(promise);\r\n    });\r\n\r\n    return new Promise( (resolve,reject) => {\r\n        busboy.on('finish', async () => {\r\n            // We still need to wait for the disk writes (saves) to complete.\r\n            await Promise.all(fileWrites)\r\n            resolve(uploads)\r\n        });\r\n        busboy.on('error', reject);\r\n    });\r\n}"},"lineCount":null}},"error":null,"hash":"ad3413bdd8550c329f185371cd699047","cacheData":{"env":{}}}