  "scripts": {
    "lint": "xo",
    "watch": "parcel src/functions.ts --target node",
    "build": "parcel build src/functions.ts --target node --detailed-report",
    "serve": "npx firebase-tools serve",
    "dev": "npm-run-all --parallel serve watch",
    "emulator": "npx firebase-tools emulators:start --only functions",
    "shell": "npx firebase-tools functions:shell",
    "start": "yarn run shell",
    "predeploy": "yarn build",
    "deploy": "npx firebase-tools deploy --only functions",
    "logs": "npx firebase-tools functions:log"
  },
  "engines": {
    "node": "10"
  },
  "xo": {
    "space": true
  },
  "devDependencies": {
